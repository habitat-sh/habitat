{
  "name": "habitat",
  "version": "0.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/chef/bldr.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "concurrent \"npm run build-js\" \"npm run build-css\"",
    "build-css": "bin/build-css",
    "build-css-watch": "npm run build-css && npm run build-css -- --watch",
    "build-js": "npm run lint-js && webpack --config webpack.conf.js",
    "build-js-watch": "npm run build-js -- --watch",
    "clean": "concurrent \"npm run clean-js\" \"npm run clean-css\"",
    "clean-css": "rm -fv assets/app.css assets/app.css.map",
    "clean-js": "rm -fv assets/app.js assets/app.js.map app**/*.js app**/*.js.map",
    "dist": "bin/dist",
    "format-js": "tsfmt --replace app**/*.ts",
    "lint": "concurrent \"npm run lint-css\" \"npm run lint-js\"",
    "lint-css": "sass-lint --verbose stylesheets**/*.scss app**/*.scss",
    "lint-css-watch": "watch 'npm run lint-css'",
    "lint-js": "tslint app**/*.ts",
    "lint-js-watch": "watch 'npm run lint-js'",
    "help": "cat README.md",
    "repl": "cd app && tsun",
    "postinstall": "typings install --no-insight",
    "start": "bin/start",
    "test": "npm run test-unit && npm run test-e2e",
    "test-watch": "concurrent \"npm run test-unit-watch\" \"npm run test-e2e-watch\"",
    "test-e2e": "webdriver-manager update && ts-node node_modules/.bin/protractor",
    "test-e2e-watch": "watch 'npm run test-e2e'",
    "test-unit": "karma start",
    "test-unit-watch": "npm run test-unit -- --no-single-run",
    "travis": "npm run test-unit"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.15",
    "ansi_up": "^1.3.0",
    "blueimp-md5": "^2.3.0",
    "bourbon": "^4.2.6",
    "bourbon-neat": "^1.7.2",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "immutable": "^3.7.6",
    "marked": "^0.3.5",
    "moment": "^2.11.2",
    "node-uuid": "^1.4.7",
    "normalize-scss": "^4.0.3",
    "octicons": "^3.5.0",
    "parse-link-header": "^0.4.1",
    "redux": "^3.0.5",
    "redux-reset": "^0.1.3",
    "redux-thunk": "^1.0.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "whatwg-fetch": "^0.11.0",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "assertion-error": "^1.0.1",
    "awesome-typescript-loader": "^0.17.0",
    "chai-as-promised": "^5.2.0",
    "concurrently": "^1.0.0",
    "deep-eql": "^0.1.3",
    "jsdom": "^8.4.0",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "^1.7.0",
    "lite-server": "^1.3.1",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.3",
    "protractor": "^3.1.0",
    "sass-lint": "^1.4.0",
    "sinon": "^1.17.3",
    "ts-node": "^0.5.5",
    "tslint": "^3.2.2",
    "tslint-loader": "^2.1.0",
    "tsun": "^0.1.2",
    "type-detect": "^1.0.0",
    "typescript": "^1.7.3",
    "typescript-formatter": "^1.2.0",
    "typings": "^0.8.1",
    "watch": "^0.17.1",
    "webpack": "^1.12.11"
  }
}
